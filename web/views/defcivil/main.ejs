                  <a href="#panel" aria-controls="panel" role="tab" data-toggle="tab" class="panel-heading darken text-uppercase">Control Panel</a>
               </li>
               <li role="presentation">
                  <a href="#map" aria-controls="map" role="tab" data-toggle="tab" class="panel-heading darken text-uppercase">Map</a>
               </li>
            </ul>
         </div>
         <div class="row well">
            <!-- Tab panes -->
            <div class="tab-content">
               <div role="tabpanel" class="tab-pane text-center" id="panel">
                  <div class="jumbotron control-panel">
                     <div class="row">
                        <div class="col-lg-6">
                           <div class="col-lg-12 well data">
                              <p class="text-uppercase text-left">number of victims</p>
                              <div class="col-lg-4">
                                 <p class="panel-strong" id="survivors">50</p>
                                 <span class="text-uppercase">survivors</span>
                              </div>
                              <div class="col-lg-4">
                                 <p class="panel-strong" id="dead">70</p>
                                 <span class="text-uppercase">dead</span>
                              </div>
                              <div class="col-lg-4">
                                 <p class="panel-strong" id="missing">10</p>
                                 <span class="text-uppercase">missing</span>
                              </div>
                           </div>
                        </div>
                        <div class="col-lg-6">
                           <div class="col-lg-12 well data">
                              <p class="text-uppercase text-left">number of specialists</p>
                              <div class="col-lg-3">
                                 <p class="panel-strong" id-"paramedics">50</p>
                                 <span class="text-uppercase">paramedics</span>
                              </div>
                              <div class="col-lg-3">
                                 <p class="panel-strong" id="ambulances">70</p>
                                 <span class="text-uppercase">ambulances</span>
                              </div>
                              <div class="col-lg-3">
                                 <p class="panel-strong" id="firemen">10</p>
                                 <span class="text-uppercase">firemen</span>
                              </div>
                              <div class="col-lg-3">
                                 <p class="panel-strong" id="policemen">10</p>
                                 <span class="text-uppercase">policemen</span>
                              </div>
                           </div>
                        </div>
                     </div>
                     <div class="row">
                        <div class="col-lg-6">
                           <div class="col-lg-12 well data">
                              <p class="text-uppercase text-left">level of fuel gases</p>
                              <div class="progress">
                                 <div class="progress-bar progress-bar-success progress-bar-striped active" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: 40%">
                                    <span class="sr-only">40% Complete (success)</span>
                                 </div>
                              </div>
                           </div>
                        </div>
                        <div class="col-lg-6">
                           <div class="col-lg-12 well data">
                              <p class="text-uppercase text-left">temperature</p>
                              <h2 class="panel-strong" id="temperature">24ËšC</h2>
                           </div>
                        </div>
                     </div>
                     <div class="row">
                        <div class="col-lg-6">
                           <div class="col-lg-12 well data">
                              <p class="text-uppercase text-left">Crisis Classification</p>
                              <p class="critical" id="classif">CRITICAL</p>
                           </div>
                        </div>
                        <div class="col-lg-6">
