<div class="container-fluid">
    <div class="row">
        <div class="panel panel-default">
            <div class="panel-heading">
                On Field Stations
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="dataTable_wrapper">
                    <table class="table table-striped table-bordered table-hover" id="dataTables-defc">
                        <thead>
                            <tr>
                                <th>ID</th>
                                <th>Station</th>
                                <th>Last Report</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% line_counter = 0 %>
                            <% content.forEach(function(station) { %>
                                <% line_counter++ %>
                                <tr class="<%= line_counter % 2 === 0? 'even' : 'odd' %> gradeX">
                                    <td><%= station.id %></td>
                                    <td><a href="#"><%= station.name %></a></td>
                                    <td><%= station.last_report %></td>
                                </tr>
                            <% }); %>
                        </tbody>
                    </table>
                </div>
                <!-- /.table-responsive -->
            </div>
        </div>
    </div>
</div>